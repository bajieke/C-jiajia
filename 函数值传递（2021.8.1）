#include<iostream>
using namespace std;

void swap(int num1, int num2)
{
	cout << "交换前：" << endl;
	cout << "num1=" << num1 << endl;
	cout << "num2=" << num2 << endl;

	int temp = num1;
	num1 = num2;
	num2 = temp;

	cout << "交换前：" << endl;
	cout << "num1=" << num1 << endl;
	cout << "num2=" << num2 << endl;

}


int main()
{
	int a = 10, b = 20;

	swap(a, b);

   //值传递时，函数形参发生改变，并不会影响实参
	//实参和形参占用的是不同的内存空间，所以形参内存改变不影响实参
	cout << a << " " << b << endl;

	

	return 0;
}
