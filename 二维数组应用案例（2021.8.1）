#include<iostream>
using namespace std;

int main()
{
	int arr[3][3] = { {100,100,100,},{90,50,100},{60,70,80} };
	int sum = 0;

	string name[3] = { "张三","李四","王五" };

	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			sum = sum + arr[i][j];
		}
		cout << name[i] << "同学的总成绩：" << sum << endl;
		sum = 0;
	}

	return 0;
}
