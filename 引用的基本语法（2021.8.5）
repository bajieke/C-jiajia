#include<iostream>
using namespace std;

int main()
{
	int a = 10;
	int& b = a;

	cout << "a=" << a << endl;
	cout << "b=" << b << endl;

	b = 22;
	cout << "a=" << a << endl;
	cout << "b=" << b << endl;

	a = 33;
	cout << "a=" << a << endl;
	cout << "b=" << b << endl;


	system("pause");
	return 0;
}
